  "license": "MIT",
  "proxy": "http://localhost:5000",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "expresse": "^1.0.2",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.0",
    "mongodb": "^3.2.7",
    "mongoose": "^5.0.12",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validator": "^9.4.1"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  },
  "keywords": []
}
